{
    "name": "ResMLP",
    "num_cross_val": 5,
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.2,
            "num_residuals_per_block": 1,
            "num_blocks": 4,
            "num_classes": 8,
            "num_initial_features": 512
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "imdb_wiki_adience",
            "num_classes": 8
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.001,
            "weight_decay": 0.01
        }
    },
    "loss": "cse",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 2,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 5,
        "tensorboard": true
    }
}