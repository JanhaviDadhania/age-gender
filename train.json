{
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.0,
            "num_residuals_per_block": 3,
            "num_blocks": 3,
            "num_classes": 1,
            "num_initial_features": 512,
            "last_activation": "LinearBounded",
            "min_bound": 0,
            "max_bound": 100
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 64,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "imdb_wiki",
            "num_classes": 1
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.0007961566078062951,
            "weight_decay": 1.0629918194937645e-08
        }
    },
    "loss": "mse",
    "metrics": [
        "accuracy",
        "accuracy_relaxed"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.07663082680255855
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}