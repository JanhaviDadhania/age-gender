{
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.2,
            "num_residuals_per_block": 4,
            "num_blocks": 0,
            "num_classes": 101,
            "num_initial_features": 512,
            "last_activation": null,
            "min_bound": null,
            "max_bound": null,
            "only_MLP": false
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 512,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "imdb_wiki_adience",
            "num_classes": 101
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.011587715829876227,
            "weight_decay": 0.054480726081375434
        }
    },
    "loss": "cse",
    "metrics": [
        "accuracy",
        "accuracy_relaxed"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.8241502276709554
        }
    },
    "trainer": {
        "epochs": 15,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}