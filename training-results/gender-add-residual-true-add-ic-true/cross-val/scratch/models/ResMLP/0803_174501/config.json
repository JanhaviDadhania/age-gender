{
    "num_cross_val": 5,
    "seeds": [
        0,
        1,
        2,
        3,
        4
    ],
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.1,
            "num_residuals_per_block": 2,
            "num_blocks": 4,
            "num_classes": 2,
            "num_initial_features": 512,
            "add_residual": true,
            "add_IC": true
        }
    },
    "data_loader": {
        "type": "GenderDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 256,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "adience",
            "num_classes": 2,
            "test_cross_val": 4,
            "training": true
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.03943508299495083,
            "weight_decay": 2.5926393015905714e-06
        }
    },
    "loss": "cse",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.18195706189458422
        }
    },
    "trainer": {
        "epochs": 30,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 5,
        "tensorboard": true
    }
}