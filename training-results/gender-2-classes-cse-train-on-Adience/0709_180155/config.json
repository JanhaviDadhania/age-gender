{
    "num_cross_val": 5,
    "seeds": [
        0,
        1,
        2,
        3,
        4
    ],
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": null,
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.45,
