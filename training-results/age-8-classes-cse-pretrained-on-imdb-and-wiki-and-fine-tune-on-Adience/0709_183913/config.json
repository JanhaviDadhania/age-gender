{
    "num_cross_val": 5,
    "seeds": [
        0,
        1,
        2,
        3,
        4
    ],
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": "training-results/age-8-classes-cse-train-on-imdb-and-wiki/model_best.pth",
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.1,
            "num_residuals_per_block": 1,
            "num_blocks": 1,
            "num_classes": 8,
            "num_initial_features": 512,
            "last_activation": null,
            "min_bound": null,
            "max_bound": null
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 65,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "Adience",
            "num_classes": 8,
            "test_cross_val": 2,
            "training": true
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.00048369271806822216,
            "weight_decay": 7.934293594590584e-07
        }
    },
    "loss": "cse",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.0006170217483403156
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}