{
    "num_cross_val": 5,
    "seeds": [
        0,
        1,
        2,
        3,
        4
    ],
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": "saved/models/ResMLP/0708_182006/model_best.pth",
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0.2,
            "num_residuals_per_block": 1,
            "num_blocks": 2,
            "num_classes": 1,
            "num_initial_features": 512,
            "last_activation": "LinearBounded",
            "min_bound": 0,
            "max_bound": 100
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 65,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "Adience",
            "num_classes": 1
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.017249321573179006,
            "weight_decay": 5.820013372709849e-07
        }
    },
    "loss": "mse",
    "metrics": [
        "accuracy",
        "accuracy_relaxed"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.0925681899206688
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}