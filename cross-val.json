{
    "num_cross_val": 5,
    "seeds": [
        0,
        1,
        2,
        3,
        4
    ],
    "name": "ResMLP",
    "n_gpu": 1,
    "amp": true,
    "checkpoint": "saved/models/ResMLP/0708_001810/model_best.pth",
    "arch": {
        "type": "ResMLP",
        "args": {
            "dropout": 0,
            "num_residuals_per_block": 3,
            "num_blocks": 1,
            "num_classes": 101,
            "num_initial_features": 512
        }
    },
    "data_loader": {
        "type": "AgeDataLoader",
        "args": {
            "data_dir": "data/",
            "batch_size": 256,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 8,
            "dataset": "Adience",
            "num_classes": 101
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.0006055098539300354,
            "weight_decay": 2.3145798905204858e-07
        }
    },
    "loss": "cse",
    "metrics": [
        "accuracy",
        "accuracy_relaxed"
    ],
    "lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 3.2413268041956336e-05
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}